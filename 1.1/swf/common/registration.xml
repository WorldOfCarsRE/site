				<confirmPassword></confirmPassword>
				<firstName></firstName>
				<lastName></lastName>
				<email></email>
				<agreedToTOU></agreedToTOU>
				<subscribe></subscribe>
			</teen>

			<!-- Adult -->
			<adult>
				<birthday></birthday>
				<username></username>
				<password></password>
