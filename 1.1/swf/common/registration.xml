<?xml version="1.0" encoding="ISO-8859-1"?>
<config>
	<!-- Tea Templates -->
	<flashAPI>
		<login>http://localhost/dxd/flashAPI/login</login>
		<createAccount>http://localhost/dxd/flashAPI/createAccount</createAccount>
		<checkUsernameAvailability>http://localhost/dxd/flashAPI/checkUsernameAvailability</checkUsernameAvailability>
		<checkUsernameApproval>http://localhost/dxd/flashAPI/checkUsernameApproval</checkUsernameApproval>
		<checkValidationStatus>http://localhost/dxd/flashAPI/checkValidationStatus</checkValidationStatus>
		<submitAliasForApproval>http://localhost/dxd/flashAPI/submitAliasForApproval</submitAliasForApproval>
		<getFamilyStructure>http://localhost/dxd/flashAPI/getFamilyStructure</getFamilyStructure>
		<verifyAccountCredentials>http://localhost/dxd/flashAPI/verifyAccountCredentials</verifyAccountCredentials>
		<linkAccountToFamily>http://localhost/dxd/flashAPI/linkAccountToFamily</linkAccountToFamily>
		<removeAccountFromFamily>http://localhost/dxd/flashAPI/removeAccountFromFamily</removeAccountFromFamily>
		<updateAccount>http://localhost/dxd/flashAPI/updateAccount</updateAccount>
		<lookupAccount>http://localhost/dxd/flashAPI/lookupAccount</lookupAccount>
		<getAllNewsletterMetadata>http://localhost/dxd/flashAPI/getAllNewsletterMetadata</getAllNewsletterMetadata>
		<notifyParent>http://localhost/dxd/flashAPI/notifyParent</notifyParent>
		<authenticateAndUpdateOptins>http://localhost/dxd/flashAPI/authenticateAndUpdateOptins</authenticateAndUpdateOptins>
		<sendChatPermissionEmails>http://localhost/dxd/flashAPI/sendChatPermissionEmails</sendChatPermissionEmails>
		<getTermsOfUseText>http://localhost/dxd/flashAPI/getTermsOfUseText</getTermsOfUseText>
		<logout>http://localhost/dxd/flashAPI/logout</logout>
		<!-- purchasing -->
		<creditCardValidation>https://register.go.com/dxd/validation/authorize</creditCardValidation>
		<lookupSubscriptions>https://register.go.com/commerce/flashapi/lookupSubscriptions</lookupSubscriptions>
		<lookupOffers>https://register.go.com/commerce/flashapi/lookupOffers</lookupOffers>
	    <purchaseOffer>https://register.go.com/commerce/flashapi/purchaseOffer</purchaseOffer>
	    <validatePaymentAccount>https://register.go.com/commerce/flashapi/validatePaymentAccount</validatePaymentAccount>
	    <lookupPaymentAccounts>https://register.go.com/commerce/flashapi/lookupPaymentAccounts</lookupPaymentAccounts>
	    <lookupShippingAddresses>https://register.go.com/commerce/flashapi/lookupShippingAddresses</lookupShippingAddresses>
	    <updateShippingAddress>https://register.go.com/commerce/flashapi/updateShippingAddressOnSubscription</updateShippingAddress>
	    <updateShippingAddressOnSubscription>https://register.go.com/commerce/flashapi/updateShippingAddressOnSubscription</updateShippingAddressOnSubscription>
	    <updatePaymentAccountOnSubscription>https://register.go.com/commerce/flashapi/updatePaymentAccountOnSubscription</updatePaymentAccountOnSubscription>
	    <paypal>https://register.go.com/commerce/paypal/redirect</paypal>
		<paypalSuccessDestination>http://worldofcars.go.com/welcome-sponsored-racer/</paypalSuccessDestination>
	    <paypalErrorDestination>http://worldofcars.go.com/sponsorship/paypal-error/</paypalErrorDestination>
		<disableAutoRenew>https://register.go.com/commerce/flashapi/disableAutoRenew</disableAutoRenew>
	    <!-- /purchasing -->
		<!-- whoami service -->
		<whoami>http://localhost/carsds/api/WhoAmIRequest</whoami>
		<!-- /whoami service -->
	</flashAPI>

	<!-- Product-specific Data -->
	<productData>
		<!-- <promotionName>disney_cars</promotionName> -->							<!-- formerly known as.. affiliate name -->
		<promotionName>World_Of_Cars_Online_Virtual_World</promotionName>
		<siteCode>US.EN.CAR</siteCode>										<!-- site code -->
		<product>cars</product>												<!-- product name in the commerce database, used by account management too -->
		<productDisplayName>The World of Cars Online</productDisplayName>	<!-- display name for paypal -->
		<fobNewsletterID>WDIGFamilySites</fobNewsletterID>					<!-- FOB emails -->
		<carsNewsletterID>Disney_WorldofCars_NLO_102008</carsNewsletterID>	<!-- Cars Newsletter -->
		<templateID>851</templateID>										<!-- Template ID -->
		<parentGuide>http://worldofcars.go.com/parents/</parentGuide>
	</productData>

